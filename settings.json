  "bracket-pair-colorizer-2.depreciation-notice": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.formatOnSave": true,
  "editor.wordWrap": "off",
  // for path intellisense
  "typescript.suggest.paths": false,
  "javascript.suggest.paths": false,
  // end path intellisense
  // EOL Stuff
  // Make it LF as default
  // "files.eol": "\n",
  // Make it CRLF as default
  "files.eol": "\r\n",
  // * Vim Settings (WIP) TODO
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  // go to the implementation of a function
  "vim.normalModeKeyBindings": [
    {
      "before": ["g", "I"],
      "commands": ["editor.action.goToImplementation"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["j"],
      "after": ["g", "j"]
    },

    {
      "before": ["k"],
      "after": ["g", "k"]
    },
    {
      "before": ["<leader>", "d"],
      "after": ["d", "d"]
    },
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    },
    {
      "before": ["K"],
      "commands": ["lineBreakInsert"],
      "silent": true
    },
    {
      "before": ["<tab>"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["<S-tab>"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["<leader>", "h"],
      "after": ["^"]
    },
    {
      "before": ["<leader>", "l"],
      "after": ["$"]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-b>": false,
    "<C-j>": false,
    "<C-p>": false
  },
  // "vim.commandLineModeKeyBindings": [], // make it false to revert to normal line numbers
  "vim.vimrc.enable": false,
  "vim.vimrc.path": "%USERPROFILE%/.vim/",
  "vim.easymotion": true,
  "vim.surround": true,

  // * VIM config end

  "settingsSync.ignoredSettings": [],
  "workbench.startupEditor": "none",
  "terminal.integrated.mouseWheelScrollSensitivity": 0.5,

